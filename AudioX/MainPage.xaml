             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="AudioX.MainPage">

    <StackLayout>
        <Button Text="Play" Clicked="Button_Clicked" HorizontalOptions="Center"/>
        <Slider x:Name="Freq" Maximum="80" Value="40"/>
        <Slider x:Name="Freq2" Maximum="80" Value="40"/>
    </StackLayout>

</ContentPage>
